{
  "expo": {
    "name": "elinity-app",
    "slug": "elinity-app",
    "scheme": "elinity",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F0C29"
    },
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#0F0C29",
      "bundleIdentifier": "com.simplisoni.elinityapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0F0C29"
      },
      "navigationBar": {
        "backgroundColor": "#0F0C29",
        "barStyle": "light-content"
      },
      "edgeToEdgeEnabled": false,
      "package": "com.simplisoni.elinityapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-asset"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9c3b23ea-75c5-450e-89c5-f1216e542db3"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/9c3b23ea-75c5-450e-89c5-f1216e542db3"
    }
  }
}
